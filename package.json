{
  "name": "poe-live-search-manager",
  "productName": "PoE Live Search Manager",
  "version": "1.16.1",
  "description": "Desktop tool for Path of Exile to make instants offers for listed items without leaving the game",
  "main": "./app/main.prod.js",
  "scripts": {
    "build": "yarn build:main && yarn build:renderer",
    "build:main": "webpack --mode production --config ./configs/webpack.main.config.js",
    "build:renderer": "webpack --mode production --config ./configs/webpack.renderer.prod.config.js",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./configs/webpack.renderer.dev.config.js",
    "dev:main": "cross-env NODE_ENV=development electron --no-sandbox --require @babel/register --require @babel/polyfill ./app/main/app.js",
    "lint": "eslint app",
    "package": "cross-env NODE_ENV=production yarn build && electron-builder",
    "package:all": "cross-env NODE_ENV=production yarn build && electron-builder --mwl",
    "package:win": "cross-env NODE_ENV=production yarn build && electron-builder build --win --x64 --publish onTag",
    "package:win:local": "cross-env NODE_ENV=production yarn build && electron-builder build --win -c.compression=store --x64",
    "package:mac": "cross-env NODE_ENV=production yarn build && electron-builder build --mac",
    "package:linux": "cross-env NODE_ENV=production yarn build && electron-builder build --linux",
    "unix:package:win": "cross-env ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true REVISION=\"$(git describe --always --tags --dirty)\" yarn package:win",
    "win:package:win": "cross-env ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true node scripts/build.js",
    "prettify": "prettier --write app/**/*.js",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "path of exile",
    "poe",
    "search",
    "trade",
    "live",
    "electron",
    "react"
  ],
  "author": "C-Hive",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/5k-mirrors/poe-live-search-manager/issues"
  },
  "homepage": "https://github.com/5k-mirrors/poe-live-search-manager",
  "engines": {
    "node": ">=11.7"
  },
  "//dependencies": {
    "material-table": "Built on top of MUI tables, supports editing, search, sorting, actions. https://material-ui.com/components/tables/#material-table"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "async-mutex": "^0.1.3",
    "bottleneck": "^2.19.2",
    "electron-store": "^4.0.0",
    "electron-unhandled": "^3.0.0",
    "electron-updater": "^4.0.14",
    "firebase": "^7.24.0",
    "js-yaml": "^3.13.1",
    "material-table": "1.62.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-firebaseui": "^3.1.2",
    "react-markdown": "^4.2.2",
    "react-router-dom": "^5.0.1",
    "remark-breaks": "^1.0.3",
    "ws": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@testing-library/react-hooks": "^3.4.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "dotenv": "^8.2.0",
    "electron": "^10.1.4",
    "electron-builder": "^20.44.4",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "javascript-obfuscator": "^2.6.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "style-loader": "^0.23.1",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0",
    "webpack-obfuscator": "^2.6.0"
  },
  "//resolutions": {
    "@babel/runtime": "https://github.com/webpack/webpack/issues/11467#issuecomment-708648622"
  },
  "resolutions": {
    "@babel/runtime": "^7.12.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/app/renderer/SetupEnzymeTests/SetupEnzymeTests.js"
    ],
    "moduleNameMapper": {
      ".+\\.(png|jpg|css|svg)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "app/**/*.js",
      "!app/main.prod.js",
      "!app/renderer.prod.js"
    ]
  }
}
